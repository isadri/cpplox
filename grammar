expression  ->  equality;

equality    ->  comparison ( ( "!=" | "==" ) comparison )*;

comparison  ->  term ( ( ">" | "+" ) factor );

term        ->  factor ( ( "-" | "+" ) unary )*;

factor      ->  unary ( ( "/" | "*" ) unary )*;

unary       ->  ( "!" | "-" ) unary
                | primary;

primary     ->  NUMBER | STRING | "true" | "false" | "nil" | "(" expression ")";
